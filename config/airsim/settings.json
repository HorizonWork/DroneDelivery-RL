{
  "SeeDocsAt": "https://microsoft.github.io/AirSim/settings/",
  "SettingsVersion": 1.5,
  "SimMode": "Multirotor",
  "LocalHostIp": "127.0.0.1",
  "ApiServerPort": 41451,
  "ClockSpeed": 1.0,
  "ViewMode": "Fpv",
  "Vehicles": {
    "Drone1": {
      "VehicleType": "SimpleFlight",
      "AutoCreate": true,
      "DefaultVehicleState": "Armed",
      "EnableCollisionPassthrough": false,
      "X": 0,
      "Y": 0,
      "Z": -5,
      "RC": {
        "RemoteControlID": 0,
        "AllowAPIWhenDisconnected": true
      },

      "Cameras": {
        "front_left": {
          "CaptureSettings": [
            { "ImageType": 0, "Width": 640, "Height": 480, "FOV_Degrees": 90 },
            { "ImageType": 3, "Width": 640, "Height": 480 }
          ],
          "X": 0.3,
          "Y": -0.05,
          "Z": 0.0,
          "Pitch": 0,
          "Roll": 0,
          "Yaw": 0
        },
        "front_right": {
          "CaptureSettings": [
            { "ImageType": 0, "Width": 640, "Height": 480, "FOV_Degrees": 90 }
          ],
          "X": 0.3,
          "Y": 0.05,
          "Z": 0.0,
          "Pitch": 0,
          "Roll": 0,
          "Yaw": 0
        },
        "front_center": {
          "CaptureSettings": [
            { "ImageType": 3, "Width": 640, "Height": 480, "FOV_Degrees": 90 }
          ],
          "X": 0.3,
          "Y": 0.0,
          "Z": 0.0,
          "Pitch": -5,
          "Roll": 0,
          "Yaw": 0
        }
      },
      "Sensors": {
        "Imu": { "SensorType": 2, "Enabled": true },
        "Gps": { "SensorType": 1, "Enabled": false },
        "Barometer": { "SensorType": 3, "Enabled": true },
        "Magnetometer": { "SensorType": 5, "Enabled": true },
        "Distance": {
          "SensorType": 5,
          "Enabled": true,
          "X": 0.2,
          "Y": 0.0,
          "Z": -0.2,
          "Yaw": 0,
          "Pitch": -90,
          "Roll": 0
        },
        "Lidar1": {
          "SensorType": 6,
          "Enabled": true,
          "NumberOfChannels": 16,
          "Range": 50,
          "PointsPerSecond": 100000
        }
      }
    }
  }
}
